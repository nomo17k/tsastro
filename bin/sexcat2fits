def parse_column_def(s):
    """
    Parse a comment line from SExtractor text catalog file.
    """
    ts = s.split()
    unit = (ts[-1][1:-1] if ts[-1].startswith('[') and ts[-1].endswith(']')
            else None)
    comment = ' '.join(ts[3:] if unit is None else ts[3:-1])
    return {'number': int(ts[1]),
            'name': ts[2],
            'comment': comment,
            'unit': unit}

def main(input, output):

    convfuncdic = {'I': int, 'J': int, 'E': float, 'D': float}

    # read data
    with open(input) as f:
        cols = []
        coldefs = []
        data_parse_mode = False
        pcoldef = None
        for line in f:
            line = line.strip()
            if not data_parse_mode:
                if line.startswith('#'):
                    coldef = parse_column_def(line)

                    # need to take care of repeated col
                    if pcoldef is not None:
                        repeat = coldef['number'] - pcoldef['number']
                        pcoldef['type'] = ('%d%s' % (repeat,
                                                     pcoldef['type'][-1]))

                    name = coldef['name']
                    if name not in paramdic:
                        raise RuntimeError('unknown output parameter (%s)'
                                           % name)
                    par = paramdic[name]

                    coldef['type'] = '1%s' % par['type']
                    coldef['disp'] = par['fmt']

                    coldefs.append(coldef)
                    pcoldef = coldef
                    continue
                else:
                    # all cols are defined as this point
                    print(coldefs)
                    for i in range(len(coldefs)):
                        cols.append([])
                    
                    data_parse_mode = True
            # data parse mode
            
            ts = line.split()
            idx = 0
            for i, coldef in enumerate(coldefs):
                type = coldef['type']
                repeat = int(type[0])
                items = []
                for j in range(repeat):
                    items.append(convfuncdic[type[-1]](ts[idx]))
                    idx += 1
                cols[i].append(items if len(items) > 1 else items[0])
    cs = []
    for i, cd in enumerate(coldefs):
        cs.append(pf.Column(name=cd['name'], format=cd['type'],
                            unit=cd['unit'], array=cols[i]))
    hdu = pf.new_table(cs)

    # add comment to column definition
    hdr = hdu.header
    for i, coldef in enumerate(coldefs):
        idx = i + 1
        repeat = int(coldef['type'][0])
        hdr.update('TTYPE%d' % idx,
                   coldef['name'],
                   comment=coldef['comment'])
        after = ('TUNIT%d' % idx if hdr.has_key('TUNIT%d' % idx)
                 else 'TFORM%d' % idx)
        hdr.update('TDISP%d' % idx,
                   coldef['disp'],
                   after=after)

    hdu.writeto(output, clobber=True)


if __name__ == '__main__':
    usage = 'usage: %prog [OPTIONS] TEXTFILE FITSFILE'
    p = OptionParser(usage=usage, version=__version__,
                     description=(__doc__.strip()).split('\n')[0])
    opts, args = p.parse_args()
    if len(args) != 2:
        p.error('invalid input arguments')
        
    input = args[0]
    output = args[1]

    main(input, output)
